<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#047A62">

    <TextView
        android:id="@+id/newname"
        android:layout_width="74dp"
        android:layout_height="41dp"
        android:layout_marginTop="52dp"
        android:text="name"
        android:textColor="#AFA975"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/passfac"
        android:layout_width="129dp"
        android:layout_height="41dp"
        android:layout_marginTop="93dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:text="pass word"
        android:textColor="#AFA975"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/newname" />

    <EditText
        android:id="@+id/nameff"
        android:layout_width="199dp"
        android:layout_height="43dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="77dp"
        android:layout_marginRight="77dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toStartOf="@+id/newname"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/add12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="159dp"
        android:layout_marginLeft="159dp"
        android:layout_marginTop="71dp"
        android:layout_marginEnd="164dp"
        android:layout_marginRight="164dp"
        android:text="ADD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passfac" />

    <Button
        android:id="@+id/back11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="159dp"
        android:layout_marginLeft="159dp"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="164dp"
        android:layout_marginRight="164dp"
        android:text="BACk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/add12" />

    <EditText
        android:id="@+id/password"
        style="@android:style/Widget.AutoCompleteTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="83dp"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toStartOf="@+id/passfac"
        app:layout_constraintTop_toBottomOf="@+id/nameff" />

</androidx.constraintlayout.widget.ConstraintLayout>